}
$(window).scroll(function() {
  if ($(window).scrollTop() &gt; 5) {
	if(!$commActive) commActive();
  }
});
  
function commActive(){
$commActive = 1;
  var bcomments = &quot;<iframe allowtransparency='allowtransparency' class='blogger-iframe-colorize blogger-comment-from-post' expr:height='data:cmtIframeInitialHeight ?: &quot;90px&quot;' expr:src='data:commentFormIframeSrc' frameborder='0' id='comment-editor' name='comment-editor' width='100%'/>&quot;;

