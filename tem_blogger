<b:tag defer='true' name='script' type='text/javascript'>
$(window).scroll(function(){
  if ($(window).scrollTop() > 30 ){
      //Kill the handler
      $(window).off("scroll");
$(window).scroll(function(){
  if ($(window).scrollTop() > 30 ){
      //Kill the handler
      $(window).off("scroll");

$( document ).ready(function() {
$commActive = 0;
if ($(window).scrollTop() &gt; 5) {
  if(!$commActive) commActive();
}
$(window).scroll(function() {
  if ($(window).scrollTop() &gt; 5) {
	if(!$commActive) commActive();
  }
});
function commActive(){
$commActive = 1;
  var bcomments = "<iframe allowtransparency='allowtransparency' class='blogger-iframe-colorize blogger-comment-from-post' expr:height='data:cmtIframeInitialHeight ?: &quot;90px&quot;' expr:src='data:commentFormIframeSrc' frameborder='0' id='comment-editor' name='comment-editor' width='100%'/>";

  $(&#39;.workcomments&#39;).html(bcomments)

  $.getScript( &quot;https://www.blogger.com/static/v1/jsbin/1769009776-comment_from_post_iframe.js&quot;, function() {
  BLOG_CMT_createIframe(&#39;<data:post.appRpcRelayPath/>&#39;);
  });

}
});
}
});
}
});
